local replacesres="**Hook Results:** "
if not pcall(hookfunction,warn,function()return "hook"end) then
    replacesres=replacesres..("üò≠ hook not present")
else
    if warn() ~= "hook" then replacesres=replacesres..("‚ö†Ô∏è hook is fake")
    else replacesres=replacesres..("‚úÖ hook present and working")
    end
end
if not pcall(function()getrenv().warn=function()return"renv"end end) then replacesres=replacesres..("\n**Other Error:** ‚ö†Ô∏è renv not present/editable") end
if not pcall(function()getgenv().warn=function()return"genv"end end) then replacesres=replacesres..("\n**Other Error:** ‚ö†Ô∏è genv not present/editable") end
replacesres=replacesres.."\n**Replace Results:** "..loadstring([[return(warn() or "üõë tampering unsuccessful")]])()
print(replacesres)


